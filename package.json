{
  "name": "react-boilerplate-typescript",
  "version": "0.1.0",
  "description": "React Webpack Babel Boilerplate Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.ts",
    "dev-manual": "webpack --config ./webpack.dev.ts --watch",
    "dev": "webpack-dev-server --config ./webpack.dev.ts --colors --open",
    "dev:hot": "npm run dev -- --hot",
    "start": "npm run dev:hot",
    "test-js": "react-scripts test --env=jsdom",
    "test": "mocha --require @babel/register --require ./test/setupEnzyme.js --require ./test/dom.js --require ignore-styles ./src/**/*.spec.ts",
    "test:watch": "npm run test -- --watch",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot -- --watch",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "test:cypress": "start-server-and-test start http://localhost:8070 cy:run",
    "ci": "npm run lint && npm run test -- --coverage && npm run test:snapshot -- -u --coverage && npm run test:cypress",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafeez-syed/react-boilerplate-typecript.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel"
  ],
  "author": "Hafeez Syed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hafeez-syed/react-boilerplate-typecript/issues"
  },
  "homepage": "https://github.com/hafeez-syed/react-boilerplate-typecript#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/enzyme": "^3.9.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/webpack": "^4.4.31",
    "@types/webpack-dev-server": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "chai": "4.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "cypress": "3.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "ignore-styles": "5.0.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-dom": "^3.2.2",
    "jsdom": "15.1.0",
    "mocha": "6.1.4",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.17.1",
    "react-axe": "^3.1.0",
    "react-hot-loader": "^4.8.5",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.9.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5",
    "webpack": "4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "core-js": "^3.0.1",
    "no-op": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
