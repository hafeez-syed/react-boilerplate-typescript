    "jsdom-global": "^3.0.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "react-axe": "^3.3.0",
    "react-hot-loader": "^4.12.12",
    "react-test-renderer": "^16.9.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
