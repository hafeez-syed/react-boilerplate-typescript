{
  "name": "react-boilerplate-typescript",
  "version": "0.1.0",
  "description": "React Webpack Babel Boilerplate Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.ts",
    "dev-manual": "webpack --config ./webpack.dev.ts --watch",
    "dev": "webpack-dev-server --config ./webpack.dev.ts --colors --open",
    "dev:hot": "npm run dev -- --hot",
    "start": "npm run dev:hot",
    "test": "jest --config ./jest.config.js",
    "test:watch": "npm run test -- --watch",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "test:cypress": "start-server-and-test start http://localhost:8070 cy:run",
    "ci": "npm run lint && npm run test -- --coverage && npm run test:cypress",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint ./src/**/*.tsx",
    "lint:fix": "eslint --fix ./src/**/*.tsx",
    "sass-lint": "sass-lint -vq src/**/*.scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run sass-lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafeez-syed/react-boilerplate-typecript.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel"
  ],
  "author": "Hafeez Syed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hafeez-syed/react-boilerplate-typecript/issues"
  },
  "homepage": "https://github.com/hafeez-syed/react-boilerplate-typecript#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.5.5",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/chai": "^4.2.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
