{
  "name": "react-boilerplate-typescript",
  "version": "0.1.0",
  "description": "React Webpack Babel Boilerplate Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.js",
    "dev-manual": "webpack --config ./webpack.dev.js --watch",
    "dev": "webpack-dev-server --config ./webpack.dev.js --open",
    "dev:hot": "npm run dev -- --hot",
    "start": "npm run dev:hot",
    "test-js": "react-scripts test --env=jsdom",
    "test": "mocha --require @babel/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles ./src/**/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot -- --watch",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "test:cypress": "start-server-and-test start http://localhost:8070 cy:run",
    "ci": "npm run lint && npm run test -- --coverage && npm run test:snapshot -- -u --coverage && npm run test:cypress",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafeez-syed/react-boilerplate-typecript.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel"
  ],
  "author": "Hafeez Syed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hafeez-syed/react-boilerplate-typecript/issues"
  },
  "homepage": "https://github.com/hafeez-syed/react-boilerplate-typecript#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "chai": "4.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "cypress": "3.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "ignore-styles": "5.0.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-dom": "^3.1.4",
    "jsdom": "15.0.0",
    "mocha": "6.1.4",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.17.0",
    "react-axe": "^3.1.0",
    "react-hot-loader": "^4.8.4",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "start-server-and-test": "^1.9.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "typescript": "^3.4.5",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
